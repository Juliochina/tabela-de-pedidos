function abreProdutosHome(onde){
    console.log(onde);
    $.support.cors = true;
    $.post(pasta_home + "ajax_home_produtos_onde.php?method=lista&returnformat=json", {pa_onde:onde}, function(res) {
        if(res.success == 1) {
            var hpQuantos = parseInt(res.hp_tot);
            var hpStr = "";
            if(hpQuantos > 0){
                for(var i = 0; i < hpQuantos; i++){
                    hpStr+= '<a class="full_home_produtos_categoria" href="'+res["lk"+i]+'">';
                    hpStr+=     '<div class="full_home_produtos_categoria_container full_home_produtos_categoria_container_p" style="background-image:url('+res["im"+i]+')"></div>';
                    hpStr+=     '<div class="full_home_produtos_categoria_texto">';
                    hpStr+=         '<div class="full_home_produtos_categoria_texto_ct full_home_produtos_categoria_texto_ct_p" style="background-color:'+res["bg"+i]+'">';
                    hpStr+=             '<span>'+res["tx"+i]+'</span>';
                    hpStr+=         '</div>';
                    hpStr+=     '</div>';
                    hpStr+= '</a>';
                }
                $("#full_home_produtos_categorias").html(hpStr);
                $("#sel_home_produtos_categoria").val(onde);
            }else{
                $("#full_home_produtos_categorias").html("<div class='hp_nenhum'>Nenhum resultado encontrado</div>");
            }
        }else{
            $("#full_home_produtos_categorias").html("<div class='hp_nenhum'>Nenhum resultado encontrado</div>");
        }
    },"json");
}

