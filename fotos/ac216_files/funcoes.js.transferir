                $('.valor_contador').each(function() {
                    var $this = $(this), countTo = $this.attr('valor_final');
                    $({countNum: $this.text()}).animate(
                        {
                            countNum: countTo
                        },{
                            duration: 7000,
                            easing: 'swing',
                            step: function() {
                              $this.text(Math.floor(this.countNum));
                        },
                        complete: function() {
                            $this.text(this.countNum);
                        }
                    });
                });
                aCh = 1;
            }
        }
    }); 

