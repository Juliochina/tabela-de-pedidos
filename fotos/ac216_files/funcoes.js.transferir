                  "-webkit-transition":"height ease-out "+(maximoLimiteBusca*33)+"ms",
                  "-moz-transition":"height ease-out "+(maximoLimiteBusca*33)+"ms",
                  "-o-transition":"height ease-out "+(maximoLimiteBusca*33)+"ms",
