            tmTransition = parseInt(maximoLimiteBusca/2.5);
        }else{
            tmTransition = maximoLimiteBusca;
        }
        if($(this).html() == "Todas"){
            $(this).html("Fechar");
            $(this).parent().find(".busca_filtros_limite").css({
                height:(maximoLimiteBusca*47)+"px",
                "-webkit-transition":"height ease-out "+(maximoLimiteBusca*33)+"ms",
                "-moz-transition":"height ease-out "+(maximoLimiteBusca*33)+"ms",
                "-o-transition":"height ease-out "+(maximoLimiteBusca*33)+"ms",
                "transition":"height ease-out "+(maximoLimiteBusca*33)+"ms"
            });
            $(this).css({
                "background-position-y":"-45px"
            });
        }else{
            if(maximoLimiteBusca > 4){
              $(this).parent().find(".busca_filtros_limite").css({
                  height:"0",
                  "-webkit-transition":"height ease-out "+(tmTransition*33)+"ms",
                  "-moz-transition":"height ease-out "+(tmTransition*33)+"ms",
                  "-o-transition":"height ease-out "+(tmTransition*33)+"ms",
                  "transition":"height ease-out "+(tmTransition*33)+"ms"
