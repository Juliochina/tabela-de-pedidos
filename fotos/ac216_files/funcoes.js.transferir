    $mask_telefone.on("keyup",function(){
        var texto = $(this).val();
        if(texto.length == 1){
            texto = "(" + texto;
        }else if(texto.length == 3){
            texto+= ") ";
        }else if(texto.length == 9){
            texto+= "-";
        }else if(texto.length == 14){
            texto = texto.replace("-","");
            var texto_limpo = "";
            for(var i = 0; i < texto.length; i++){
                if(i == 9){
                    texto_limpo+= "-";
                }
                texto_limpo+= texto.charAt(i);
            }
            texto = texto_limpo;
        }else if(texto.length == 15){
            texto = texto.replace("-","");
            var texto_limpo = "";
            for(var i = 0; i < texto.length; i++){
                if(i == 10){
                    texto_limpo+= "-";
                }
                texto_limpo+= texto.charAt(i);
            }
            texto = texto_limpo;
        }

        for(var i = 0; i < 10; i++){
            texto = texto.replace(".","-");
            texto = texto.replace("--","-");
            texto = texto.replace("((","(");
            texto = texto.replace("))",")");
            texto = texto.replace("  "," ");
        }

        $(this).val(texto);
    });

    var $mask_cep = $(".mask_cep");
    $mask_cep.unbind("keyup");
    $mask_cep.on("keyup",function(){
        var texto = $(this).val();
        if(texto.length == 5){
            texto+= "-";
        }else if(texto.length == 9){
            var textoXCEP = texto;
            for(var i = 0; i < 19; i++){
                textoXCEP = textoXCEP.replace("-","");
            }

            $.get("https://republicavirtual.com.br/web_cep.php?cep="+ textoXCEP +"&formato=json", function(res) {
                if(res.resultado == 1) {
                    var cepUF = res.uf;
                    var cepCidade = res.cidade;
                    var cepBairro = res.bairro;
                    var cepTipo = res.tipo_logradouro;
                    var cepEndereco = res.logradouro;
                    console.log(cepUF + ' | ' + cepCidade + ' | ' + cepBairro + ' | ' + cepTipo + ' ' + cepEndereco);
                    $('.cep_uf').val(cepUF);
                    $('.cep_endereco').val(cepTipo + ' ' + cepEndereco);
                    $('.cep_bairro').val(cepBairro);
                    $('.cep_cidade').val(cepCidade);
                }else{
                    console.log('CEP Não encontrado.');
                }
            },"json");

        }

        for(var i = 0; i < 10; i++){
            texto = texto.replace(".","-");
            texto = texto.replace("--","-");
            texto = texto.replace("((","(");
            texto = texto.replace("))",")");
            texto = texto.replace(" ","");
        }

        $(this).val(texto);
    });
});

