            if($("#"+elementoID).css("opacity") == "0"){
                if(scrollTop > offsetTop){
                    $("#"+elementoID).css({
                        "-moz-transform": "translate(0px, 0)",
                        "-webkit-transform": "translate(0px, 0)",
